{
  "name": "glc",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "npx playwright install && playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint src/main/webapp/src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format:check": "prettier --check src/main/webapp/src/",
    "format": "prettier --write src/main/webapp/src/",
    "clean": "find . -name node_modules -type d -prune -o -name cache -type d -o -name dist -type d -o -name playwright-report -type d -o -name test-results -type d | xargs rm -rf",
    "prepare": "husky install",
    "predev": "dotenv-checker --schema .env.example --env .env.local"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.27",
    "vue-i18n": "9.13.1",
    "vue-router": "^4.3.2",
    "vuetify": "^3.6.5"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "3.0.6",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@playwright/test": "^1.44.0",
    "@rushstack/eslint-patch": "^1.10.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.isempty": "^4.4.9",
    "@types/node": "^20.11.17",
    "@types/xml2js": "^0.4.14",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "axios": "^1.7.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "dotenv-checker": "^1.1.5",
    "eslint": "^8.57.0",
    "eslint-plugin-playwright": "^1.6.0",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "lodash.debounce": "^4.0.8",
    "lodash.isempty": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.77.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.19",
    "vue3-toastify": "^0.2.1",
    "xml2js": "^0.6.2"
  },
  "packageManager": "yarn@4.2.2"
}
