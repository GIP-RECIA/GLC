{
  "name": "src-main-webapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "clean": "find . -name node_modules -type d -prune -o -name cache -type d -o -name dist -type d -o -name playwright-report -type d -o -name test-results -type d | xargs rm -rf",
    "prepare": "husky install",
    "predev": "dotenv-checker --schema .env.example --env .env.local"
  },
  "dependencies": {
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-i18n": "9.2.2",
    "vue-router": "^4.2.4",
    "vuetify": "^3.3.10"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@intlify/unplugin-vue-i18n": "^0.12.2",
    "@playwright/test": "^1.36.2",
    "@rushstack/eslint-patch": "^1.3.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.isempty": "^4.4.7",
    "@types/node": "^18.17.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "axios": "^1.4.0",
    "dotenv-checker": "^1.1.5",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "lodash.debounce": "^4.0.8",
    "lodash.isempty": "^4.4.0",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "regenerator-runtime": "^0.13.11",
    "sass": "^1.64.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.33.0",
    "vue-toastification": "v2.0.0-rc.5",
    "vue-tsc": "^1.8.8"
  },
  "packageManager": "yarn@3.6.1"
}
