{
  "name": "glc",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "npx playwright install && playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint src/main/webapp/src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format:check": "prettier --check src/main/webapp/src/",
    "format": "prettier --write src/main/webapp/src/",
    "clean": "find . -name node_modules -type d -prune -o -name cache -type d -o -name dist -type d -o -name playwright-report -type d -o -name test-results -type d | xargs rm -rf",
    "prepare": "husky install",
    "predev": "dotenv-checker --schema .env.example --env .env.local"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.14",
    "vue-i18n": "9.9.0",
    "vue-router": "^4.2.5",
    "vuetify": "^3.5.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "3.0.5",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@playwright/test": "^1.41.1",
    "@rushstack/eslint-patch": "^1.6.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.isempty": "^4.4.9",
    "@types/node": "^20.11.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "axios": "^1.6.5",
    "date-fns": "^3.2.0",
    "dotenv": "^16.3.2",
    "dotenv-checker": "^1.1.5",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isempty": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.69.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-vuetify": "^2.0.1",
    "vitest": "^1.2.1",
    "vue-toastification": "v2.0.0-rc.5",
    "vue-tsc": "^1.8.27"
  },
  "packageManager": "yarn@4.0.2"
}
