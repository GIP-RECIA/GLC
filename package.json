{
  "name": "glc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "npx playwright install && playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint src/main/webapp/src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format:check": "prettier --check src/main/webapp/src/",
    "format": "prettier --write src/main/webapp/src/",
    "clean": "find . -name node_modules -type d -prune -o -name cache -type d -o -name dist -type d -o -name playwright-report -type d -o -name test-results -type d | xargs rm -rf",
    "prepare": "husky install",
    "predev": "dotenv-checker --schema .env.example --env .env.local"
  },
  "dependencies": {
    "@vueuse/core": "^10.4.1",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-i18n": "9.4.1",
    "vue-router": "^4.2.5",
    "vuetify": "^3.3.17"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "latest-3",
    "@intlify/unplugin-vue-i18n": "^1.2.0",
    "@playwright/test": "^1.38.1",
    "@rushstack/eslint-patch": "^1.4.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.isempty": "^4.4.7",
    "@types/node": "^18.17.19",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "axios": "^1.5.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "dotenv-checker": "^1.1.5",
    "eslint": "^8.50.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "lodash.debounce": "^4.0.8",
    "lodash.isempty": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.68.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.34.5",
    "vue-toastification": "v2.0.0-rc.5",
    "vue-tsc": "^1.8.13"
  },
  "packageManager": "yarn@3.6.3"
}
